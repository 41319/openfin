{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "overrides": [
    {
      "files": ["*.html"],
      "options": {
        "htmlWhitespaceSensitivity": "ignore",
        "jsxBracketSameLine": true
      }
    },
    {
      "files": ["*.ts"],
      "options": {
        "singleQuote": false
      }
    }
  ],
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "none",
  "useTabs": false
}
