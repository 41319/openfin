{
  "devtools_port": 9090,
  "licenseKey": "openfin-demo-license-key",
  "runtime": {
    "arguments": "--v=1 --inspect",
    "version": "24.98.69.7"
  },
  "platform": {
    "uuid": "integrate-with-excel",
    "icon": "http://localhost:8080/favicon.ico",
    "autoShow": false,
    "providerUrl": "http://localhost:8080/platform/provider.html",
    "preventQuitOnLastWindowClosed": true,
    "permissions": {
      "System": {
        "downloadAsset": true,
        "launchExternalProcess": true,
        "openUrlWithBrowser": {
          "enabled": true,
          "protocols": [
            "mailto"
          ]
        }
      }
    },
    "defaultWindowOptions": {
      "permissions": {
        "System": {
          "openUrlWithBrowser": {
            "enabled": true,
            "protocols": [
              "mailto"
            ]
          }
        }
      }
    }
  },
  "snapshot": {
    "windows": []
  },
  "shortcut": {
    "company": "OpenFin",
    "description": "An way of showing examples of what OpenFin can do.",
    "icon": "http://localhost:8080/favicon.ico",
    "name": "Integrate With Excel - v7.0",
    "target": [
      "desktop",
      "start-menu"
    ]
  },
  "appAssets": [
    {
      "alias": "excel-interop-example.xlsx",
      "version": "0.0.3",
      "src": "http://localhost:8080/assets/excel-interop-example.zip",
      "target": "excel-interop-example.xlsx",
      "forceDownload": true
    }
  ],
  "customSettings": {
    "homeProvider": {
      "id": "integrate-with-excel",
      "title": "Integrate With Excel",
      "icon": "http://localhost:8080/favicon.ico"
    },
    "integrationProvider": {
      "integrations": [
        {
          "id": "excel",
          "icon": "http://localhost:8080/assets/excel.svg",
          "title": "Excel Provider",
          "enabled": true,
          "moduleUrl": "http://localhost:8080/js/integrations/excel.bundle.js",
          "data": {
            "assets": [
              {
                "title": "Excel Interop Example",
                "description": "Demonstrate interop with Excel workbook",
                "workbook": "excel-interop-example.xlsx",
                "worksheet": "Sheet1",
                "cellHandlers": [
                  {
                    "cell": "$B$3",
                    "type": "instrument",
                    "color": "red"
                  },
                  {
                    "cell": "$B$4",
                    "type": "instrument",
                    "color": "orange"
                  },
                  {
                    "cell": "$B$5",
                    "type": "instrument",
                    "color": "yellow"
                  },
                  {
                    "cell": "$B$6",
                    "type": "instrument",
                    "color": "green"
                  },
                  {
                    "cell": "$B$7",
                    "type": "instrument",
                    "color": "blue"
                  },
                  {
                    "cell": "$B$8",
                    "type": "instrument",
                    "color": "purple"
                  }
                ]
              }
            ]
          }
        }
      ]
    }
  }
}